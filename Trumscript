<body>
    <!-- Nút toggle di chuyển được -->
    <div class="toggle-wrapper toggle-draggable" id="draggableToggle">
        <div class="toggle-switch">
            <input type="checkbox" id="mainToggle" checked>
            <span class="toggle-slider"></span>
        </div>
        <div class="toggle-label">KÉO ĐỂ DI CHUYỂN</div>
    </div>
    
    <!-- Menu chính -->
    <div class="script-container" id="scriptMenu">
        <div class="script-header">
            <div class="script-title">
                <div class="logo">TS</div>
                <h1>Trùm Script</h1>
            </div>
            
            <div class="toggle-wrapper">
                <div class="toggle-switch">
                    <input type="checkbox" id="menuToggle" checked>
                    <span class="toggle-slider pulse-animation"></span>
                </div>
                <div class="toggle-label">BẬT/TẮT</div>
            </div>
        </div>
        
        <div class="command-menu">
            <div class="command-list">
                <div class="command-item" onclick="toggleCommand(1)">
