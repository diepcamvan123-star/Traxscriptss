    <script>
        // Biến toàn cục
        let isDragging = false;
        let dragOffset = { x: 0, y: 0 };
        let toggleElement = document.getElementById('draggableToggle');
        let scriptMenu = document.getElementById('scriptMenu');
        let mainToggle = document.getElementById('mainToggle');
        let menuToggle = document.getElementById('menuToggle');
        
        // Khởi tạo vị trí toggle
        toggleElement.style.left = '20px';
        toggleElement.style.top = '20px';
        
        // Xử lý nút bật/tắt menu chính
        mainToggle.addEventListener('change', function() {
            if (this.checked) {
                scriptMenu.classList.remove('menu-hidden');
                console.log('Menu đã BẬT');
            } else {
                scriptMenu.classList.add('menu-hidden');
                console.log('Menu đã TẮT');
            }
        });
        
        // Xử lý nút bật/tắt menu trong header
        menuToggle.addEventListener('change', function() {
            if (this.checked) {
                scriptMenu.classList.remove('menu-hidden');
                mainToggle.checked = true;
                console.log('Menu đã BẬT');
            } else {
                scriptMenu.classList.add('menu-hidden');
                mainToggle.checked = false;
                console.log('Menu đã TẮT');
            }
        });
        
        // Xử lý bật/tắt từng command
        function toggleCommand(commandId) {
            const statusIndicator = document.getElementById(`status${commandId}`);
            const commandItem = document.querySelectorAll('.command-item')[commandId - 1];
            
            if (statusIndicator.classList.contains('status-active')) {
                statusIndicator.classList.remove('status-active');
                commandItem.style.borderColor = '#3a3a6a';
                console.log(`Command ${commandId}: TẮT`);
            } else {
                statusIndicator.classList.add('status-active');
                commandItem.style.borderColor = '#00ff80';
                console.log(`Command ${commandId}: BẬT`);
            }
        }
        
        // Xử lý kéo thả cho toggle
        toggleElement.addEventListener('mousedown', startDrag);
        document.addEventListener('mousemove', drag);
        document.addEventListener('mouseup', stopDrag);
        
        // Xử lý cho màn hình cảm ứng
        toggleElement.addEventListener('touchstart', function(e) {
            startDrag(e.touches[0]);
        });
        document.addEventListener('touchmove', function(e) {
            if (isDragging) {
                drag(e.touches[0]);
            }
        });
        document.addEventListener('touchend', stopDrag);
        
        function startDrag(e) {
            isDragging = true;
            const rect = toggleElement.getBoundingClientRect();
            
            dragOffset.x = e.clientX - rect.left;
            dragOffset.y = e.clientY - rect.top;
            
            toggleElement.style.cursor = 'grabbing';
            if (e.preventDefault) e.preventDefault();
        }
        
        function drag(e) {
            if (!isDragging) return;
            
            // Giới hạn di chuyển trong màn hình
            const maxX = window.innerWidth - toggleElement.offsetWidth;
            const maxY = window.innerHeight - toggleElement.offsetHeight;
            
            // Tính toán vị trí mới
            let newX = e.clientX - dragOffset.x;
            let newY = e.clientY - dragOffset.y;
            
            // Giới hạn biên
            newX = Math.max(0, Math.min(newX, maxX));
            newY = Math.max(0, Math.min(newY, maxY));
            
            // Áp dụng vị trí mới
            toggleElement.style.left = `${newX}px`;
            toggleElement.style.top = `${newY}px`;
            
            if (e.preventDefault) e.preventDefault();
        }
        
        function stopDrag() {
            isDragging = false;
            toggleElement.style.cursor = 'move';
        }
        
        // Thêm phím tắt cho các command
        document.addEventListener('keydown', function(e) {
            // Chỉ xử lý khi không đang nhập liệu
            if (e.target.tagName === 'INPUT' || e.target.tagName === 'TEXTAREA') return;
            
            // Ctrl/Cmd + phím tắt
            if (e.ctrlKey || e.metaKey) {
                switch(e.key.toLowerCase()) {
                    case 'b': toggleCommand(1); break;
                    case 'l': toggleCommand(2); break;
                    case 'e': toggleCommand(3); break;
                    case 's': toggleCommand(4); break;
                    case 'm': toggleCommand(5); break;
                    case 'r': toggleCommand(6); break;
                    case 'h': 
                        // Ẩn/hiện menu với Ctrl+H
                        scriptMenu.classList.toggle('menu-hidden');
                        mainToggle.checked = !scriptMenu.classList.contains('menu-hidden');
                        menuToggle.checked = !scriptMenu.classList.contains('menu-hidden');
                        break;
                }
            }
            
            // Phím tắt không cần Ctrl (All Executor)
            if (!e.ctrlKey && !e.metaKey && !e.altKey && !e.shiftKey) {
                switch(e.key.toLowerCase()) {
                    case '1': toggleCommand(1); break;
                    case '2': toggleCommand(2); break;
                    case '3': toggleCommand(3); break;
                    case '4': toggleCommand(4); break;
                    case '5': toggleCommand(5); break;
                    case '6': toggleCommand(6); break;
                    case ' ': 
                        // Space để ẩn/hiện menu
                        scriptMenu.classList.toggle('menu-hidden');
                        mainToggle.checked = !scriptMenu.classList.contains('menu-hidden');
                        menuToggle.checked = !scriptMenu.classList.contains('menu-hidden');
                        break;
                }
            }
        });
        
        // Khởi tạo trạng thái ban đầu
        window.addEventListener('DOMContentLoaded', function() {
            console.log('Trùm Script Mini đã sẵn sàng!');
            console.log('Phím tắt: 1-6 (bật/tắt command), Space (ẩn/hiện menu)');
            console.log('Ctrl+H: Ẩn/hiện menu, Ctrl+B/L/E/S/M/R: Phím tắt command');
        });
        
        // Xử lý responsive cho mobile
        function checkMobile() {
            if (window.innerWidth <= 480) {
                // Điều chỉnh cho mobile
                if (parseInt(toggleElement.style.left) < 10) {
                    toggleElement.style.left = '10px';
                }
                if (parseInt(toggleElement.style.top) < 10) {
                    toggleElement.style.top = '10px';
                }
            }
        }
        
        window.addEventListener('resize', checkMobile);
        checkMobile();
    </script>
</body>
</html>
